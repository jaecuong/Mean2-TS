{
    "name": "mean2-ts",
    "version": "1.0.0",
    "description": "Mean2 With Typescript",
    "main": "index.js",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "start": "node dist/server/app.js",
        "predev": "tsc -p src/server",
        "frontend": "ng serve --open",
        "backend": "concurrently \"tsc -w -p src/server\" \"nodemon\"",
        "dev": "concurrently \"ng serve -pc proxy.conf.json --open\" \"tsc -w -p src/server\" \"nodemon\"",
        "prod": "ng build -aot -prod && tsc -p src/server && node dist/server/app.js",
        "lint": "ng lint",
        "clean:node_modules": "del /q .\\node_modules\\*.* && for /d %i in (.\\node_modules\\*.*) do @rmdir /s /q \"%i\" ",
        "clean:dist": "del /q .\\dist\\*.* && for /d %i in (.\\dist\\*.*) do @rmdir /s /q \"%i\" ",
        "clean": "npm run clean:node_modules && npm run clean:dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jaecuong/Mean2-TS.git"
    },
    "author": "Pham Nguyen Phi",
    "license": "ISC",
    "dependencies": {
        "@angular/common": "^4.3.3",
        "@angular/compiler": "^4.3.3",
        "@angular/core": "^4.3.3",
        "@angular/forms": "^4.3.3",
        "@angular/http": "^4.3.3",
        "@angular/platform-browser": "^4.3.3",
        "@angular/platform-browser-dynamic": "^4.3.3",
        "@angular/router": "^4.3.3",
        "@types/node": "^8.0.20",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.17.2",
        "bootstrap": "^3.3.7",
        "composable-middleware": "^0.3.0",
        "compression": "^1.7.0",
        "continuation-local-storage": "^3.2.0",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "dotenv": "^4.0.0",
        "express": "^4.15.4",
        "express-jwt": "^5.3.0",
        "express-session": "^1.15.5",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.1",
        "jsonwebtoken": "^7.4.2",
        "lodash": "^4.17.4",
        "mkdirp": "^0.5.1",
        "mongoose": "^4.11.6",
        "morgan": "^1.8.2",
        "passport": "^0.3.2",
        "passport-jwt": "^2.2.1",
        "passport-local": "^1.0.0",
        "path": "^0.12.7",
        "rxjs": "^5.4.2",
        "shelljs": "^0.7.8",
        "tether": "^1.4.0",
        "winston": "^2.3.1",
        "zone.js": "^0.8.16"
    },
    "devDependencies": {
        "@angular/cli": "1.2.7",
        "@angular/compiler-cli": "^4.3.3",
        "codelyzer": "^3.1.2",
        "concurrently": "^3.5.0",
        "nodemon": "^1.11.0",
        "ts-node": "^3.3.0",
        "tslint": "^5.6.0",
        "typescript": "^2.4.2"
    },
    "bugs": {
